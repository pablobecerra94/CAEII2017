<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/content_main"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
tools:context="overhead.caeii2017.MainActivity"
tools:showIn="@layout/app_bar_main"
    android:background="@drawable/logo"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/twitter"
        android:id="@+id/twitterImage"
        android:layout_alignTop="@+id/facebookImage"
        android:layout_centerHorizontal="true" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/aareii"
        android:layout_marginLeft="23dp"
        android:layout_marginStart="23dp"
        android:id="@+id/aareiiImage"
        android:layout_alignTop="@+id/twitterImage"
        android:layout_toRightOf="@+id/twitterImage"
        android:layout_toEndOf="@+id/twitterImage" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/facebook"
        android:layout_marginBottom="18dp"
        android:id="@+id/facebookImage"
        android:layout_alignParentBottom="true"
        android:layout_toLeftOf="@+id/twitterImage"
        android:layout_toStartOf="@+id/twitterImage"
        android:layout_marginRight="20dp"
        android:layout_marginEnd="20dp" />

</RelativeLayout>


